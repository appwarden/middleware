{
  "name": "@appwarden/middleware",
  "version": "1.1.5",
  "description": "Instantly shut off access your app deployed on Cloudflare or Vercel",
  "private": false,
  "sideEffects": true,
  "type": "module",
  "license": "MIT",
  "author": "Appwarden <support@appwarden.io>",
  "main": "./build/index.js",
  "types": "./build/index.d.ts",
  "keywords": [
    "appwarden",
    "nextjs",
    "remixjs",
    "cloudflare",
    "web3",
    "monitoring"
  ],
  "scripts": {
    "prepare": "node scripts/prepare-semantic-release.cjs",
    "dev": "pnpm build --watch",
    "deploy:staging": "pnpm build && wrangler deploy -e staging",
    "release": "NODE_ENV=production pnpm build && node scripts/prepare-package.cjs && semantic-release",
    "build": "tsup",
    "check": "tsc --noEmit",
    "test": "vitest"
  },
  "dependencies": {
    "@cloudflare/next-on-pages": "1.13.5",
    "@upstash/redis": "^1.34.0",
    "@vercel/edge-config": "^1.4.0",
    "zod": "^3"
  },
  "peerDependencies": {
    "next": ">=13"
  },
  "peerDependenciesMeta": {
    "next": {
      "optional": true
    }
  },
  "devDependencies": {
    "@cloudflare/workers-types": "4.20250214.0",
    "@semantic-release/changelog": "6.0.3",
    "@semantic-release/exec": "7.1.0",
    "@semantic-release/npm": "12.0.1",
    "cheerio": "1.0.0",
    "miniflare": "4.20250508.0",
    "semantic-release": "24.2.3",
    "tsup": "8.4.0",
    "typescript": "5.8.2",
    "undici": "5.28.5",
    "vite-tsconfig-paths": "5.1.4",
    "vitest": "^3.1.3"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  }
}
